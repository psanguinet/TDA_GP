@model Modelo.Models.LiquidacionDeSueldo

@{
    ViewBag.Title = "CrearLiquidación";
}

<h2>Crear Liquidación</h2>

@using (Html.BeginForm("CreateLiq", "LiquidacionSueldo")) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)


    <fieldset>

        <div class="editor-label">
            @Html.DisplayName("Mes a Liquidar")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Mes)
            @Html.ValidationMessageFor(model => model.Mes)
        </div>
        <div class="editor-label">
            @Html.DisplayName("Año a Liquidar")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Anio)
            @Html.ValidationMessageFor(model => model.Anio)
        </div>

        <p>
            <input type="submit" value="Crear Liquidacion" id="Create" />
        </p>

    </fieldset>
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}