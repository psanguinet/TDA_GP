

@model Modelo.Models.LiquidacionDeSueldo

@{
    ViewBag.Title = "VerLiquidacionSueldo";
}

<h2>Ver Liquidación</h2>

@using (Html.BeginForm("Index", "LiquidacionSueldo")) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)


    <fieldset>

        <div class="editor-label">
            @Html.DisplayName("Mes de Liquidación a ver")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Mes)
            @Html.ValidationMessageFor(model => model.Mes)
        </div>
        <div class="editor-label">
            @Html.DisplayName("Año de Liquidación a ver")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Anio)
            @Html.ValidationMessageFor(model => model.Anio)
        </div>

        <p>
            <input type="submit" value="Ver Liquidación" id="Index" />
        </p>

    </fieldset>
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}